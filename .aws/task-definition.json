{
    "family": "fogata-td",
    "containerDefinitions": [
        {
            "name": "fogata-webapp",
            "image": "914642698270.dkr.ecr.us-east-1.amazonaws.com/fogata:76c5c2d33472d636deff8ae0e0187c4059d9a12e",
            "cpu": 0,
            "portMappings": [
                {
                    "name": "fogata-webapp-80-tcp",
                    "containerPort": 3000,
                    "hostPort": 3000,
                    "protocol": "tcp",
                    "appProtocol": "http"
                }
            ],
            "essential": true,
            "environment": [],
            "mountPoints": [],
            "volumesFrom": [],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-create-group": "true",
                    "awslogs-group": "/ecs/fogata-td",
                    "awslogs-region": "us-east-1",
                    "awslogs-stream-prefix": "ecs"
                }
            }
        }
    ],
    "taskRoleArn": "arn:aws:iam::914642698270:role/ecsTaskExecutionRole",
    "executionRoleArn": "arn:aws:iam::914642698270:role/ecsTaskExecutionRole",
    "networkMode": "awsvpc",
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "512",
    "memory": "1024",
    "runtimePlatform": {
        "cpuArchitecture": "X86_64",
        "operatingSystemFamily": "LINUX"
    }
}
